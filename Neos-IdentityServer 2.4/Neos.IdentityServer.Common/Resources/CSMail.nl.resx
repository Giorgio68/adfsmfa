                &lt;td id = "i5" style = "padding: 0; padding-top: 10px; font-family: ' Segoe UI ', Tahoma, Verdana, Arial, sans-serif; font-size: 14px; color: #2a2a2a;" &gt;Secundair e-mailadres : &lt;a dir = "ltr" id = "iEmail" class = "link" style = "color: #2672ec; text-decoration: none;" &gt; {2} &lt;/td&gt;
            &lt;/tr&gt;
            &lt;tr&gt;
                &lt;td id = "i6" style = "padding: 0; padding-top: 10px; font-family: ' Segoe UI ', Tahoma, Verdana, Arial, sans-serif; font-size: 14px; color: #2a2a2a;" &gt;Mobiel telefoonnummer : &lt;a dir = "ltr" id = "iPhone" class = "link" style = "color: #2672ec; text-decoration: none;" &gt; {3} &lt;/td&gt;
            &lt;/tr&gt;
            &lt;tr&gt;
                &lt;td id = "i7" style = "padding: 0; padding-top: 10px; font-family: ' Segoe UI ', Tahoma, Verdana, Arial, sans-serif; font-size: 14px; color: #2a2a2a;" &gt;Standaard authentificatie methode : &lt;a dir="ltr" id="iMethod" class="link" style="color:#2672ec;text-decoration:none;" &gt; {4} &lt;/td&gt;
            &lt;/tr&gt;
            &lt;tr&gt;
                &lt;td id = "i8" style = "padding: 0; padding-top: 25px; font-family: ' Segoe UI ', Tahoma, Verdana, Arial, sans-serif; font-size: 14px; color: #2a2a2a;" &gt;Regards, &lt;/td&gt;
            &lt;/tr&gt;
            &lt;tr&gt;
                &lt;td id = "i9" style = "padding: 0; font-family: ' Segoe UI ', Tahoma, Verdana, Arial, sans-serif; font-size: 14px; color: #2a2a2a;" &gt;Accounts team {0}&lt;/td&gt;
